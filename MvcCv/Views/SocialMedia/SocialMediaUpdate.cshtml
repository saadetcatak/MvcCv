@model MvcCv.Models.Entity.TblSocialMedia
@{
    ViewBag.Title = "SocialMediaUpdate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>Sosyal Medya Düzenleme Sayfası</h4>
<br />
<form method="post">

    @Html.Label("Sosyal Medya")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />

    <br />
    <button class="btn btn-outline-success">Güncelle</button>
</form>

